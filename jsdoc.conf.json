{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./doc/",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "light",
      "title": "t.ex-graph-converter",
      "displayModuleHeader": true,
      "footer": "Philip Raschke Copyright Â© 2022"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}